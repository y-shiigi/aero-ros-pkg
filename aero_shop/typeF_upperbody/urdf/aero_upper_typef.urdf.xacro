    <limit lower="-1.535" upper="0.261" effort="200" velocity="0.5" />
    <dynamics damping="0.2" friction="0" />
  </joint>
  <xacro:gazebo_transmission joint_name="l_shoulder_p_joint" />

  <joint name="l_shoulder_r_joint" type="revolute">
