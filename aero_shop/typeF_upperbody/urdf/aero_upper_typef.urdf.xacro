      <geometry>
        <mesh filename="package://aero_description/meshes/typeF_upperbody/RARM_LINK5_mesh.dae" scale="1 1 1" />
      </geometry>
    </visual> </xacro:unless>
    <xacro:unless value="${disable_mesh}"> <collision>
