    <child  link="l_elbow_link"/>
    <origin xyz="0.035 0 -0.1625" rpy="0 -0 0 "/>
    <axis   xyz="0 1 0"/>
    <!-- from -171 to 0 degrees-->
    <limit lower="-3.0" upper="0" effort="100" velocity="1.0" />
