      <geometry>
        <mesh filename="package://aero_description/meshes/typeF_upperbody/RARM_LINK4_mesh.dae" scale="1 1 1" />
      </geometry>
    </visual> </xacro:unless>
    <xacro:unless value="${disable_mesh}"> <collision>
      <origin xyz="-0.035 0 -0.035" rpy="0 -0 0"/>
      <geometry>
        <mesh filename="package://aero_description/meshes/typeF_upperbody/RARM_LINK4_mesh.dae" scale="1 1 1" />
      </geometry>
    </collision> </xacro:unless>
    <inertial>
      <mass value="0.251" />
      <origin xyz="-0.0422 -0.0002 -0.0061" rpy="0 -0 0"/>
      <inertia ixx="0.0005" ixy="0" ixz="0.0001" iyy="0.0005" iyz="0" izz="0.0002"/>
    </inertial>
  </link>
