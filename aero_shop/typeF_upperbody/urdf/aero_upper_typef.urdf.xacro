    <dynamics damping="0.2" friction="0" />
  </joint>
  <xacro:gazebo_transmission joint_name="l_shoulder_r_joint" />

  <joint name="l_shoulder_y_joint" type="revolute">
    <parent link="l_shoulder_center"/>
