    <inertial>
      <mass value="6.7" />
      <origin xyz="0.001 0 0.197" rpy="0 -0 0"/>
<!--
      <inertia ixx="0.334" ixy="0" ixz="0.0035" iyy="0.308" iyz="0.0001" izz="0.0497"/>
-->
