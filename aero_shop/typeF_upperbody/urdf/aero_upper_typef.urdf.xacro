      <geometry>
        <mesh filename="package://aero_description/meshes/typeF_upperbody/NECK_LINK1_mesh.dae" scale="1 1 1" />
      </geometry>
    </collision> </xacro:unless>
    <inertial>
      <mass value="0.275" />
      <origin xyz="-0.0003 0.0003 0.0439" rpy="0 -0 0"/>
      <inertia ixx="0.0002" ixy="0" ixz="0" iyy="0.0002" iyz="0" izz="0.0001"/>
