      </geometry>
    </collision> </xacro:unless>
    <inertial>
      <mass value="0.685" />
      <origin xyz="0.0047 0 0.0711" rpy="0 -0 0"/>
      <inertia ixx="0.0018" ixy="0" ixz="0.0001" iyy="0.0017" iyz="0" izz="0.0014"/>
    </inertial>
  </link>
  <xacro:gazebo_link_reference link_name="head_link" />

  <link name="r_shoulder_link">
    <xacro:unless value="${disable_mesh}"> <visual>
      <origin xyz="0 0 0" rpy="0.174532925 -0 0"/>
      <geometry>
        <mesh filename="package://aero_description/meshes/typeF_upperbody/RARM_LINK1_mesh.dae" scale="1 1 1" />
      </geometry>
    </visual> </xacro:unless>
    <xacro:unless value="${disable_mesh}"> <collision>
      <origin xyz="0 0 0" rpy="0.174532925 -0 0"/>
      <geometry>
        <mesh filename="package://aero_description/meshes/typeF_upperbody/RARM_LINK1_mesh.dae" scale="1 1 1" />
      </geometry>
    </collision> </xacro:unless>
    <inertial>
      <mass value="0.738" />
      <origin xyz="0.0001 0.0499 0.0044" rpy="0 -0 0"/>
      <inertia ixx="0.002" ixy="0" ixz="0" iyy="0.0003" iyz="0.0002" izz="0.0019"/>
    </inertial>
  </link>
  <xacro:gazebo_link_reference link_name="r_shoulder_link" />

  <link name="r_shoulder_center">
    <xacro:unless value="${disable_mesh}"> <visual>
      <origin xyz="0 0 0" rpy="0 -0 0"/>
      <geometry>
        <mesh filename="package://aero_description/meshes/typeF_upperbody/RARM_LINK2_mesh.dae" scale="1 1 1" />
      </geometry>
    </visual> </xacro:unless>
    <xacro:unless value="${disable_mesh}"> <collision>
      <origin xyz="0 0 0" rpy="0 -0 0"/>
      <geometry>
        <mesh filename="package://aero_description/meshes/typeF_upperbody/RARM_LINK2_mesh.dae" scale="1 1 1" />
      </geometry>
    </collision> </xacro:unless>
    <inertial>
      <mass value="0.834" />
      <origin xyz="-0.0007 0.0096 -0.0768" rpy="0 -0 0"/>
      <inertia ixx="0.0011" ixy="0" ixz="0" iyy="0.0011" iyz="0" izz="0.0004"/>
    </inertial>
  </link>
