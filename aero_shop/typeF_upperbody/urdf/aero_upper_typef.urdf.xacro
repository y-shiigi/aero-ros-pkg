    </inertial>
  </link>
  <xacro:gazebo_link_reference link_name="head_base_link" />

  <link name="head_link">
    <xacro:unless value="${disable_mesh}"> <visual>
      <origin xyz="0 0 0" rpy="0 -0 0"/>
      <geometry>
